    scanf("%d %d %d",&a,&b,&c);
    if ( (a+b>c) && (a+c>b) && (c+b>a) )
        {
            if(a==b && b==c && a==c)
                {
                    printf("\nTringle formed is an Equilateral triangle");
                }
            else if(a==b && b!=c)
                {
                    printf("\nTrinangle in Isoceles triangle");
                }
            else if(a!=b && b!=c && a!=c)
                {
